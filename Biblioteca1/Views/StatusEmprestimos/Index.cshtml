@model IEnumerable<Biblioteca1.StatusEmprestimo>

@{
    ViewBag.Title = "Status de Emprestimos";
}

<h2>Lista de status de emprestimos</h2>

<p>
    @Html.ActionLink("Adicionar status", "Create")
</p>
<table class="table">
    <tr>
        <th>
           Status
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Label)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Excluir", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
